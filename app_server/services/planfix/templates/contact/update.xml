<?xml version="1.0" encoding="UTF-8"?>
<request method="contact.update">
  <account>{{ account }}</account>
  <sid>{{ sid }}</sid>
  <silent>{{ silent }}</silent>
  <contact>
    <id>{{ id }}</id>
    <userid>{{ userid }}</userid>
    <general>{{ general }}</general>
    <name>{{ name }}</name>
    <midName>{{ midName }}</midName>
    <lastName>{{ lastName }}</lastName>
    <template>{{ template }}</template>
    <post>{{ post }}</post>
    <email>{{ email }}</email>
    <phones>
        <phone>
            <number>{{ number }}</number>
            <typeId>{{ typeId }}</typeId>
            <typeName>{{ typeName }}</typeName>
        </phone>
    </phones>
    <secondaryEmails>
        <email>{{ email }}</email>
    </secondaryEmails>
    <address>{{ address }}</address>
    <description>{{ description }}</description>
    <sex>{{ sex }}</sex>
    <site>{{ site }}</site>
    <skype>{{ skype }}</skype>
    <facebook>{{ facebook }}</facebook>
    <vk>{{ vk }}</vk>
    <icq>{{ icq }}</icq>
    <birthdate>{{ birthdate }}</birthdate>
    <lang>{{ lang }}</lang>
    <canBeWorker>{{ canBeWorker }}</canBeWorker>
    <canBeClient>{{ canBeClient }}</canBeClient>
    <group>
      <id></id>
    </group>
    <customData>
      <customValue>
        <id></id>
        <value></value>
      </customValue>
    </customData>
    <responsible>
      <users>
        <id></id>
        <id></id>
      </users>
      <groups>
        <id></id>
        <id></id>
        <!-- ... -->
      </groups>
    </responsible>
  </contact>
  <signature>{{ signature }}</signature>
</request>